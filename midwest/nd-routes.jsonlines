{"route_name": "West Overhang", "grade": {"YDS": "5.8", "French": "5b", "Ewbanks": "16", "UIAA": "VI-", "ZA": "15", "British": "HVS 4c"}, "safety": "", "type": {"sport": true}, "fa": "unknown", "description": ["Climb the west face through the concrete overhang. Avoid using the sandstone corners. "], "location": ["Trestle 2, West face"], "protection": ["None"], "metadata": {"left_right_seq": "999999", "parent_lnglat": [-96.77594, 46.87946], "parent_sector": "Trestle 2", "mp_route_id": "120451241", "mp_sector_id": "120450984", "mp_path": "Shake Dome|Trestle 2"}}
