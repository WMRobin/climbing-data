{"route_name": "Frodosynthesis", "grade": {"YDS": "V1-2", "Font": "5"}, "safety": "", "type": {"boulder": true}, "fa": "Chris Deal", "description": ["Balance will you get you to the top of this interesting line, A line with a great view"], "location": ["Secreat Area"], "protection": ["one pad"], "metadata": {"left_right_seq": "999999", "parent_lnglat": [-103.7035, 46.86904], "parent_sector": "Square Butte Bouldering", "mp_route_id": "111342065", "mp_sector_id": "111341935", "mp_path": "Square Butte Bouldering"}}
