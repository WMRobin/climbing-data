{"route_name": "Fred Flinstone", "grade": {"YDS": "5.9", "French": "5c", "Ewbanks": "17", "UIAA": "VI", "ZA": "17", "British": "HVS 5a"}, "safety": "", "type": {"sport": true}, "fa": "Mike McNeil", "description": ["\"WILMA!\""], "location": ["After walking past the Big Black Face for about 30 meters, start looking up the hill to the right to find a path to a blocky, black formation with two routes in it. Fred Flinstone is just a minute hike beyond these routes."], "protection": ["Lots of quickdraws."], "metadata": {"left_right_seq": "0", "parent_lnglat": [-103.89038, 44.29596], "parent_sector": "In Between Routes a.k.a \"The Flintstones Wall\"", "mp_route_id": "107292449", "mp_sector_id": "107292445", "mp_path": "Spearfish Canyon|Sunshine|In Between Routes a.k.a \"The Flintstones Wall\""}}
{"route_name": "save me jebus", "grade": {"YDS": "V-easy", "Font": "3"}, "safety": "", "type": {"boulder": true}, "fa": "tyler durden or me ?", "description": ["off the sit start then climb crack"], "location": ["hang left at semi flat areaafter the first big steep push up sunshine trail"], "protection": ["pad"], "metadata": {"left_right_seq": "999999", "parent_lnglat": [-103.89038, 44.29596], "parent_sector": "Hungry Homer Boulder", "mp_route_id": "106034568", "mp_sector_id": "106034564", "mp_path": "Spearfish Canyon|Sunshine|Hungry Homer Boulder"}}
